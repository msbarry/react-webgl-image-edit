{
  "name": "my-react-boilerplate",
  "version": "1.0.0",
  "description": "My react boilerplate",
  "private": true,
  "scripts": {
    "clean": "rimraf dist",
    "test": "NODE_ENV=test mocha -G --compilers js:babel-core/register,css:./scripts/css-null-compiler src/**/*.test.js",
    "test:watch": "npm test -- --watch",
    "lint": "eslint . && echo 'no lint errors!'",
    "lint:watch": "npm run lint -s; onchange src scripts *.js .eslint* -- npm run lint -s",
    "start": "parallelshell 'npm run lint:watch' 'npm run serve:dev'",
    "preserve:prod": "npm run build",
    "serve:prod": "NODE_ENV=production node scripts/serve.js",
    "serve:dev": "NODE_ENV=development node scripts/serve.js",
    "prebuild": "npm run clean -s && npm run lint -s && npm test -s",
    "build": "NODE_ENV=production webpack --config webpack.config.js --progress --colors -p"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.2.3",
    "babel-core": "^6.3.26",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.1",
    "babel-preset-stage-0": "^6.3.13",
    "babel-root-import": "^3.1.0",
    "css-loader": "^0.23.1",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^3.0.0",
    "eslint-plugin-react": "^3.14.0",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^1.7.0",
    "ip": "^1.1.0",
    "jsdom": "^7.2.2",
    "mocha": "^2.3.4",
    "normalize.css": "^3.0.3",
    "onchange": "^2.0.0",
    "opener": "^1.4.1",
    "parallelshell": "^2.0.0",
    "postcss-import": "^7.1.3",
    "postcss-loader": "^0.8.0",
    "postcss-reporter": "^1.3.0",
    "react": "^0.14.5",
    "react-addons-test-utils": "^0.14.6",
    "react-dom": "^0.14.5",
    "rimraf": "^2.5.0",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-dev-server": "^1.14.0",
    "webpack-hot-middleware": "^2.6.0"
  },
  "dependencies": {}
}
